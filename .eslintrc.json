{
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "vue",
    "prettier",
    "@typescript-eslint"
  ],
  "extends": [
    "google",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "plugin:vue/recommended",
    "prettier",
    "prettier/vue",
    "prettier/@typescript-eslint"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "project": "./tsconfig.json"
  },
  "rules": {
    "new-cap": "off",
    "require-jsdoc": "off"
  }
}
